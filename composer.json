{
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },

  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "Tests\\": "src/test/"
    }
  },

  "scripts": {
    "test": "phpunit --coverage-clover=coverage.xml"
  },

  "minimum-stability": "stable",
  "prefer-stable": true
}